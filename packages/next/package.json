{
  "name": "@qstate/next",
  "version": "1.0.0",
  "description": "Next.js adapter for QState",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "/dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@qstate/core": "workspace:*",
    "@qstate/react": "workspace:*"
  },
  "peerDependencies": {
    "@qstate/core": "*",
    "@qstate/react": "*",
    "next": "^13.0.0 || ^15.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^15.0.7",
    "@types/react": "^18.3.26",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.38.0",
    "jsdom": "^24.1.3",
    "next": "^15.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "react",
    "nextjs",
    "state",
    "query",
    "url"
  ],
  "license": "MIT"
}